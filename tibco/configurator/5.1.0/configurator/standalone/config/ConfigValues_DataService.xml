			<ConfValue description="Pooled Data Source Driver Class Name" name="Pooled Data Source Driver Class Name" propname="datasourceDriverClassName" sinceVersion="5.0" visibility="Basic">
				<ConfEnum>
					<EnumValue default="true" selected="true" value="org.postgresql.Driver"/>
					<EnumValue value="oracle.jdbc.driver.OracleDriver"/>
				</ConfEnum>
			</ConfValue>
			<ConfValue description="Pooled Data Source Username" name="Pooled Data Source Username" propname="orderDsUsername" sinceVersion="5.0" visibility="Basic">
				<ConfString default="aff_oms" value="orderdsuserll"/>
