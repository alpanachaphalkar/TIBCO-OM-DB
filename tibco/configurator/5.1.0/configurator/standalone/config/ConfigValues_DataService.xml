			<ConfValue description="Pooled Data Source Password" name="Pooled Data Source Password" propname="orderDsPassword" sinceVersion="5.0" visibility="Basic">
				<ConfString default="aff_oms" isPassword="true" value="orderdsuserll"/>
			</ConfValue>
			<ConfValue description="Pooled Data Source URL" name="Pooled Data Source URL" propname="orderDsUrl" sinceVersion="5.0" visibility="Basic">
				<ConfEnum>
					<EnumValue  selected="true" default="true" value="jdbc:postgresql://localhost:5432/orderdsdbll?currentSchema=orderdsschemall"/>
