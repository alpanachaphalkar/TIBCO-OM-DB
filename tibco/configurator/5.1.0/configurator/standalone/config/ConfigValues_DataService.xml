			<ConfValue description="Interval delay in millisecs between Status Message retries" name="Status Message Retry Interval" propname="com.tibco.af.oms.statusmessage.redelivery.delay" sinceVersion="5.0" visibility="Basic">
				<ConfNum default="3000" value="3000"/>
			</ConfValue>
