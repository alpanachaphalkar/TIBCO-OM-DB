			</ConfValue>
			<ConfValue description="Orchestrator Web Service Cancel Order URL" name="Orchestrator Web Service Cancel Order URL" propname="orderOrchCancelOrderUrl" sinceVersion="5.0" visibility="Basic">
				<ConfString default="http://localhost:9093/v1/order/cancel" value="http://localhost:9093/v1/order/cancel"/>
			</ConfValue>
			<ConfValue description="Orchestrator Web Service Withdraw Order URL" name="Orchestrator Web Service Withdraw Order URL" propname="orderOrchWithdrawOrderUrl" sinceVersion="5.0" visibility="Basic">
				<ConfString default="http://localhost:9093/v1/order" value="http://localhost:9093/v1/order"/>
			</ConfValue>
			<ConfValue description="Orchestrator Web Service Bulk Order Action URL" name="Orchestrator Web Service Bulk Order Action URL" propname="orderOrchBulkActionUrl" sinceVersion="5.0" visibility="Basic">
				<ConfString default="http://localhost:9093/v1/orders/bulkaction" value="http://localhost:9093/v1/orders/bulkaction"/>
			</ConfValue>
			<ConfValue description="Orchestrator Web Service Plan Preview URL" name="Orchestrator Web Service Plan Preview URL" propname="orderOrchPlanPreviewUrl" sinceVersion="5.0" visibility="Basic">
				<ConfString default="http://localhost:9093/v1/planPreview" value="http://localhost:9093/v1/planPreview"/>
			</ConfValue>
			<ConfValue description="Orchestrator Web Service Purge Order URL" name="Orchestrator Web Service Purge Order URL" propname="orderPurgeOrderUrl" sinceVersion="5.0" visibility="Basic">
				<ConfString default="http://localhost:9093/v1/order/purgeOrder" value="http://localhost:9093/v1/order/purgeOrder"/>
