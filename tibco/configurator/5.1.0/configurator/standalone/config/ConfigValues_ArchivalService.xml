				<ConfString default="http://localhost:9093/v1/planitem/errorHandlerreply" value="http://localhost:9093/v1/planitem/errorHandlerreply"/>
			</ConfValue>
			<ConfValue description="Orchestrator Web Service BulkError Handler Order URL" name="Orchestrator Web Service BulkError Handler Order URL" propname="orderBulkErrorHandlerreplyUrl" sinceVersion="5.0" visibility="Basic">
				<ConfString default="http://localhost:9093/v1/planitem/bulkErrorHandlerreply" value="http://localhost:9093/v1/planitem/bulkErrorHandlerreply"/>
			</ConfValue>
