<?xml version="1.0" encoding="UTF-8"?>
<Configuration description="The Initial Configuration Setup for Archival Service" displayName="Archival Service" name="Archival Service" xmlns="http://www.tibco.com/af/config/confbeans/1.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.tibco.com/af/config/confbeans/1.0">
	<Cluster>
		<Category description="Archival Engine Configuration" name="Archival Engine Configuration" visibility="Basic">
			<ConfValue description="Logging Configuration File Path" isHotDeployable="true" name="Logging Configuration File Path" propname="logging.config" readonly="false" sinceVersion="5.0" visibility="Basic">
				<ConfString default="config/logback_arch.xml" value="config/logback_arch.xml"/>
			</ConfValue>
			<ConfValue description="Enable Order Purging Post Order Completion" isHotDeployable="true" name="Enable Order Purging Post Order Completion" propname="enablePurgeCompleteOrder" readonly="false" sinceVersion="5.0" visibility="Basic">
				<ConfBool default="false" value="false"/>
			</ConfValue>			
			<ConfValue description="" isHotDeployable="true" name="Order UDF Search Keys" propname="com.tibco.archival.order.udf.searchkeys" readonly="false" sinceVersion="5.0" visibility="Basic">
				<ConfList>
					<ConfListString value="OrderUDF"/> 
				</ConfList>
			</ConfValue>
			<ConfValue description="" isHotDeployable="true" name="Order Line UDF Search Keys" propname="com.tibco.archival.orderline.udf.searchkeys" readonly="false" sinceVersion="5.0" visibility="Basic">
				<ConfList>
					<ConfListString value="test"/> 
				</ConfList>
			</ConfValue>
			<ConfValue description="" isHotDeployable="true" name="Order Executable States" propname="com.tibco.archival.order.states" readonly="false" sinceVersion="5.0" visibility="Basic">
				<ConfList>
					<ConfListString value="Execution"/> 
					<ConfListString value="Blocked"/> 
					<ConfListString value="Complete"/>
					<ConfListString value="Cancelled"/>
					<ConfListString value="Withdrawn"/>
					<ConfListString value="OPD"/> 
					<ConfListString value="Suspended"/>
					<ConfListString value="Cancelled"/>
					<ConfListString value="Prequalificationfailed"/>
					<ConfListString value="Feasibility"/> 
				</ConfList>
			</ConfValue>
			<ConfValue description="Orchestrator Interaction Mode" isHotDeployable="true" name="Orchestrator Interaction Mode" propname="orchInteractionMode" readonly="false" sinceVersion="5.0" visibility="Basic">

					<ConfString default="true" value="JMS"/>

			</ConfValue>
			<ConfValue description="Application name to show on actuator info endpoint" isHotDeployable="false" name="Application Name" propname="info.app.name" readonly="false" sinceVersion="5.0" visibility="Basic">
				<ConfString default="Archival Service" value="Archival Service"/>
			</ConfValue>
		</Category>
		<Category description="Archival Database Configurations" name="Archival Database Configurations" visibility="Basic">
			<ConfValue description="Data Source Driver Name" isHotDeployable="true" name="Data Source Driver Name" propname="datasourceDriverClassName" readonly="false" sinceVersion="5.0" visibility="Basic">
				<ConfEnum>
					<EnumValue default="true" selected="true" value="org.postgresql.Driver"/>
					<EnumValue value="oracle.jdbc.driver.OracleDriver"/>
				</ConfEnum>
			</ConfValue>
			<ConfValue description="Data Source URL" isHotDeployable="false" name="Data Source URL" propname="archivalDsUrl" readonly="false" sinceVersion="5.0" visibility="Basic">
				<ConfEnum>
					<EnumValue selected="true" default="true" value="jdbc:postgresql://localhost:5432/archivaldbll?currentSchema=archivalschemall"/>
					<EnumValue  value="jdbc:oracle:thin:@//localhost:1521/orcl.apac.tibco.com"/>
				</ConfEnum>
			</ConfValue>
			<ConfValue description="Data Source Username" isHotDeployable="true" name="Data Source Username" propname="archivalDsUsername" readonly="false" sinceVersion="5.0" visibility="Basic">
				<ConfString default="archivaluserll" value="archivaluserll"/>
			</ConfValue>
			<ConfValue description="Data Source Password" isHotDeployable="true" name="Data Source Password" propname="archivalDsPassword" readonly="false" sinceVersion="5.0" visibility="Basic">
				<ConfString default="archivaluserll" isPassword="true"  value="archivaluserll"/>
			</ConfValue>
			<ConfValue description="Hibernate Dialect" isHotDeployable="true" name="Hibernate Dialect" propname="hibernateDialect" readonly="false" sinceVersion="5.0" visibility="Advanced">
				<ConfEnum>
					<EnumValue default="true" selected="true" value="org.hibernate.dialect.PostgreSQLDialect"/>
					<EnumValue  value="org.hibernate.dialect.Oracle10gDialect"/>
				</ConfEnum>
			</ConfValue>
			<ConfValue description="Enable Hibernate Metadata Defaults" isHotDeployable="true" name="Enable Hibernate Metadata Defaults" propname="hibernateDsDefaults" readonly="false" sinceVersion="5.0" visibility="Advanced">
				<ConfBool default="false" value="false"/>
			</ConfValue>
			<ConfValue description="Enable Hibernate Show Queries" isHotDeployable="true" name="Enable Hibernate Show Queries" propname="archivalHibernateShowSql" readonly="false" sinceVersion="5.0" visibility="Advanced">
				<ConfBool default="false" value="false"/>
			</ConfValue>
			<ConfValue description="Number of connections that will be established when the connection pool is started." isHotDeployable="true" name="Connection Pool Size" propname="archivalDsInitialSize" readonly="false" sinceVersion="5.0" visibility="Advanced">
				<ConfString default="10" value="10"/>
			</ConfValue>
			<ConfValue description="Maximum number of milliseconds that the pool will wait (when there are no available connections and the getMaxActive has been reached) for a connection to be returned before throwing an exception." isHotDeployable="true" name="Max Wait Time" propname="archivalDsMaxWait" readonly="false" sinceVersion="5.0" visibility="Advanced">
				<ConfString default="30000" value="30000"/>
			</ConfValue>
			<ConfValue description="Maximum number of active connections that can be allocated from this pool at the same time." isHotDeployable="true" name="Max Active Time" propname="archivalDsMaxActive" readonly="false" sinceVersion="5.0" visibility="Advanced">
				<ConfString default="100" value="100"/>
			</ConfValue>
			<ConfValue description="Maximum number of connections that should be kept in the idle pool." isHotDeployable="true" name="Max Connections in the Idle Pool" propname="archivalDsMaxIdle" readonly="false" sinceVersion="5.0" visibility="Advanced">
				<ConfString default="100" value="100"/>
			</ConfValue>
			<ConfValue description="Minimum number of established connections that should be kept in the pool at all times." isHotDeployable="true" name="Min Connections in the Pool" propname="archivalDsMinIdle" readonly="false" sinceVersion="5.0" visibility="Advanced">
				<ConfString default="10" value="10"/>
			</ConfValue>
			<ConfValue description="Pooled Data Source Validation Query" name="Pooled Data Source Validation Query" propname="datasourceValidationQuery" sinceVersion="1.1" visibility="Advanced">
				<ConfEnum>
					<EnumValue  default="true" selected="true"  value="SELECT 1"/>
					<EnumValue value="SELECT 1 from dual"/>
				</ConfEnum>
			</ConfValue>
			<ConfValue description="Pooled Data Source Test OnBorrow" name="Pooled Data Source Test OnBorrow" propname="archivalDsTestOnBorrow" sinceVersion="1.1" visibility="Advanced">
				<ConfBool default="true" value="true"/>
			</ConfValue>
			<ConfValue description="Pooled Data Source Validation Interval" name="Pooled Data Source Validation Interval" propname="archivalDsValidationInterval" sinceVersion="1.1" visibility="Advanced">
				<ConfString default="5000" value="5000"/>
			</ConfValue>
		</Category>
		<Category description="Orchestrator Configuration" name="Orchestrator Configuration" visibility="Basic">
